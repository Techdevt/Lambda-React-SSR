{
  "name": "lambda_test",
  "customName": false,
  "customRole": false,
  "handler": "lambda_test/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "custom": {
    "excludePatterns": [],
    "envVars": []
  },
  "endpoints": [
    {
      "path": "lambda_test/{id}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "notVelocity": "plainString",
          "partVelocity": "plainString $input.params('id') otherString",
          "fullVelocity": "$input.params('id')", 
          "fullUnknownVelocity": "$thisVarDoesNotExist",
          "partUnknownVelocity": "plainString $thisVarDoesNotExist otherString"
        }
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    },
    {
      "path": "lambda_test",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": {
        "application/json": {
          "payload": "$input.json('$')",
          "id_json": "$input.json('$.id')",
          "id_path": "$input.path('$').id",
          "true": true,
          "false": false
        }
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": {
            "application/json": ""
          }
        }
      }
    }
  ],
  "events": []
}